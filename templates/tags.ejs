<html lang="en">
<head>
  <%- include('./includes/head', { siteTitle: themeConfig.siteName }) %>
</head>
<body>
  <div class="antialiased">
    <div class="max-w-5xl mx-auto px-6 md:px-8">
      <%- include('./includes/header') %>
      <div class="mb-20 md:flex mt-10 lg:mt-6 md:border-t md:border-gray-400 md:pt-2 lg:border-t-0 lg:pt-0">
        <div class="w-full">
          <div class="mt-6 lg:mt-10 border-b border-gray-400">
            <div class="border-t border-gray-400 py-6 sm:py-8 lg:py-10">
              <% tags.forEach((tag, index) => { %>
                <a href="<%= tag.link %>" class="py-1 px-4 border-gray-300 border text-sm inline-block text-gray-700 hover:text-gray-900 rounded mr-2 mb-2"><%= tag.name %> [<%= tag.count %>]</a>
              <% }); %>
            </div>
          </div>
        </div>
      </div>
      <%- include('./includes/footer') %>
    </div>
  </div>
</body>
</html>